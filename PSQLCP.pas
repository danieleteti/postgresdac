Procedure Convert(FromValue, ToValue : PChar; iLen : LongInt; CharFrom:TConvertChar;CharTo:TConvertChar);
var
  uni:word;
  I  : integer;
  OrigByte,
  TranByte : byte;
  FromTable,
  ToTable: PCharTable;
  Ptr : PChar;
begin
  FromTable := GetCharTable(CharFrom);
  ToTable := GetCharTable(CharTo);
  Move(FromValue^, ToValue^, iLen);
  if FromTable = nil then Exit;
  if ToTable = nil then Exit;
  Ptr := ToValue;
  while Ptr^ <> #0 do
  begin
      OrigByte := ord(Ptr^);
      if OrigByte > 127 then
      begin
         uni:=FromTable[OrigByte];
         if uni>127 then
         begin
            TranByte := OrigByte;
            for I:=128 to 255 do
                if ToTable[I]=uni then
                begin
                    TranByte:=I;
                    break;
                end;
            if OrigByte <> TranByte then
               OrigByte:=TranByte;
         end  else OrigByte:=lo(uni);
      end;
      Ptr^ := Char(Ord(OrigByte));
      Inc(Ptr);
  end;
end;

Function GetCPfromName(const Name:string):TConvertChar;
var
  s: string;
begin
  s := UpperCase(Name);
  if (Pos('LATIN1', s) = 1) or (Pos('CZECH', s) = 1) or (Pos('GERMAN1', s) = 1) then
    Result := cc8859_1
  else if (Pos('LATIN2', s) = 1) or (Pos('HUNGARIAN', s) = 1) or (Pos('CROAT', s) = 1) then
    Result := cc8859_2
  else if Pos('ESTONIA', s) = 1 then
    Result := cc8859_4
  else if Pos('LATIN5', s) = 1 then
    Result := cc8859_5
  else if Pos('GREEK', s) = 1 then
    Result := cc8859_7
  else if Pos('HEBREW', s) = 1 then
    Result := cc8859_8
  else if (Pos('DANISH', s) = 1) or (Pos('SWE7', s) = 1) then
    Result := cc8859_10
  else if (Pos('WIN1250', s) = 1) then
    Result := ccCP1250
  else if (Pos('CP1251', s) = 1) or (Pos('WIN1251', s) = 1) or (Pos('WIN1251UKR', s) = 1) then
    Result := ccCP1251
  else if (Pos('CP1257', s) = 1) then
    Result := ccCP1257
  else if (Pos('KOI8-RU', s) = 1) or (Pos('KOI8-UKR',s ) = 1) then
    Result := ccKOI8_R
  else
    Result := ccUndefine;
end;

